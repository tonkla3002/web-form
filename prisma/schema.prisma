generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Log {
  id        Int      @id @default(autoincrement())
  date      String
  mealName  String
  veggies   String // We will store JSON string here
  vitamins  String // We will store JSON string here
  createdAt DateTime @default(now())
}
